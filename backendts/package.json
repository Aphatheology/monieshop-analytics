  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.5",
    "@types/unzipper": "^0.10.10",
    "ts-node": "^10.9.2",
